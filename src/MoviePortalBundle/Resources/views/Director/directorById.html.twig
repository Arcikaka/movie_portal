{% extends '@MoviePortal/base.html.twig' %}
{% block content %}
<div>
    <h3> {{ director.name }} {{ director.surname }}</h3>
</div>
<div>
    Date of Birth : {{ director.dateOfBirth|date("m/d/Y") }}
</div>
<div>
    {% for movie in director.movies %}
    <a href="{{ path('movie_by_id', {'id' : movie.id}) }}">{{ movie.title }}</a>
    {% endfor %}
</div>
{% endblock %}